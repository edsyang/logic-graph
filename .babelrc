{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": { 
          "browsers": ["chrome >= 50"],
          "node": "current"
        },
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime", {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": false,
        "regenerator": true
      }
    ]
  ]
}